{
  "name": "@syncano/cli",
  "version": "0.15.0",
  "description": "Command Line Interface for Syncano",
  "author": "Maciej Kucharz",
  "license": "MIT",
  "keywords": [
    "syncano",
    "baas",
    "frontend",
    "backend for react",
    "javascript backend",
    "backend as a service",
    "serverless",
    "serverless framework",
    "serverless applications",
    "serverless solutions"
  ],
  "engines": {
    "node": ">= 10"
  },
  "main": "./lib/cli.js",
  "homepage": "http://syncano.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Syncano/syncano-node.git"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "syncano-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "bugs": {
    "url": "https://github.com/Syncano/syncano-node/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.10.4",
    "@syncano/core": "0.15.0",
    "@syncano/template-project-empty": "0.13.0",
    "@syncano/template-project-hello": "0.15.0",
    "@syncano/template-socket-es6": "0.15.0",
    "@syncano/template-socket-es6-validate": "0.15.0",
    "@syncano/template-socket-vanilla": "0.15.0",
    "@syncano/validate": "0.15.0",
    "@types/form-data": "^2.2.1",
    "@types/listr": "^0.14.0",
    "@types/md5-file": "^4.0.0",
    "@types/mkdirp": "^0.5.2",
    "analytics-node": "^3.1.1",
    "archiver": "^3.0.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.3",
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.1",
    "create-error": "^0.3.1",
    "debug": "^4.1.1",
    "es6-template-strings": "^2.0.1",
    "find-key": "^2.1.3",
    "form-data": "^2.3.3",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.3",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.12.0",
    "klaw-sync": "^6.0.0",
    "listr": "^0.14.2",
    "listr-verbose-renderer": "^0.5.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "md5-file": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-dir": "^0.1.17",
    "node-walkup": "^1.1.1",
    "oclif": "^1.12.8",
    "ora": "^3.4.0",
    "pretty-bytes": "^5.2.0",
    "raven": "^2.6.4",
    "readdirp": "^3.0.2",
    "semver": "^6.1.1",
    "source-map": "^0.7.3",
    "stack-trace": "0.0.10",
    "strip-ansi": "^5.2.0",
    "tslib": "^1.9.3",
    "unzip2": "^0.2.5",
    "validator": "^11.0.0",
    "walkdir": "^0.4.0",
    "watchr": "^4.0.1",
    "ws": "^7.0.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/plugin-legacy": "^1.1.3",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@syncano/test-tools": "0.15.0",
    "@types/archiver": "^3.0.0",
    "@types/bluebird": "^3.5.25",
    "@types/debug": "4.1.4",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "6.0.3",
    "@types/js-yaml": "^3.11.4",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.119",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.0.8",
    "@types/ora": "^3.0.0",
    "@types/pretty-bytes": "^5.1.0",
    "@types/raven": "^2.5.1",
    "@types/semver": "^6.0.0",
    "@types/source-map-support": "^0.5.0",
    "@types/strip-ansi": "^5.2.1",
    "@types/validator": "^10.11.1",
    "@types/ws": "^6.0.1",
    "@types/yauzl": "^2.9.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "console.mute": "^0.3.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^6.1.4",
    "mock-fs": "^4.7.0",
    "npm-cli-login": "^0.1.1",
    "nyc": "^14.1.1",
    "prettier": "^1.15.2",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.3.2",
    "sinon-test": "^2.4.0",
    "source-map-support": "^0.5.10",
    "standard": "^12.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.12.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "prepare": "npm run build",
    "postinstall": "scripts/postinstall || exit 0",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "clean": "rm -rf lib/",
    "_unit": "mocha 'tests/unit/*.test.js' --require babel-register",
    "unit": "exit 0",
    "unit:watch": "npm run unit -- -w",
    "test": "concurrently \"npm:test:unit\" \"npm:test:e2e:group:*\"",
    "test:e2e:init": "npm run test:e2e-single tests/e2e/init.test-e2e.js",
    "test:e2e:anonymous": "npm run test:e2e-single tests/e2e/anonymous.test-e2e.js",
    "test:e2e:deploy": "npm run test:e2e-single tests/e2e/deploy.test-e2e.js",
    "test:e2e:config": "npm run test:e2e-single tests/e2e/config.test-e2e.js",
    "test:e2e:registry": "npm run test:e2e-single tests/e2e/registry.test-e2e.js",
    "test:e2e:hosting": "npm run test:e2e-single tests/e2e/hosting.test-e2e.js",
    "test:e2e:socket": "npm run test:e2e-single tests/e2e/socket.test-e2e.js",
    "test:e2e:instance": "npm run test:e2e-single tests/e2e/instance.test-e2e.js",
    "test:e2e-single": "mocha --require babel-register --timeout 100000 --slow 8000",
    "test:commands": "nyc --extension .ts mocha --forbid-only \"test/commands/**/*.test.ts\"",
    "test:unit": "npm run unit",
    "lint": "standard --fix --env mocha",
    "format": "prettier --write --single-quote --no-semi --no-bracket-spacing *.js {src,test}/**/*.js"
  },
  "publishConfig": {
    "access": "public"
  }
}
