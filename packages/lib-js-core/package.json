{
  "name": "@syncano/core",
  "version": "0.12.1",
  "description": "A library to intereact with the Syncano API on a server side.",
  "main": "./lib/index.js",
  "types": "./lib/syncano-core.d.ts",
  "author": "Syncano Inc.",
  "license": "MIT",
  "repository": "https://github.com/syncano/syncano-node/tree/master/packages/lib-js-core",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "syncano",
    "core",
    "socket",
    "endpoint"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf ./lib",
    "build": "npm run build:ts && npm run build:dts",
    "build:ts": "npx tsc",
    "build:dts": "dts-bundle --name '@syncano/core' --main ./lib/index.d.ts --out syncano-core.d.ts",
    "test": "jest",
    "lint": "tslint {src,test}/**/*.ts"
  },
  "dependencies": {
    "debug": "3.1.0",
    "form-data": "2.3.2",
    "lodash.get": "4.4.2",
    "lodash.merge": "4.6.1",
    "lodash.set": "4.3.2",
    "node-fetch": "2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^23.3.1",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.5.7",
    "@types/node-fetch": "^2.1.2",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cross-env": "5.2.0",
    "dts-bundle": "^0.7.3",
    "isparta": "4.1.0",
    "jest": "^23.4.2",
    "nock": "^9.4.4",
    "rimraf": "2.6.2",
    "should": "13.2.3",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      ".*": "ts-jest"
    },
    "testRegex": "(/test/unit/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
