{
  "name": "@syncano/core",
  "version": "0.12.1",
  "description": "A library to intereact with the Syncano API on a server side.",
  "main": "./lib/index.js",
  "types": "./lib/main.d.ts",
  "author": "Syncano Inc.",
  "license": "MIT",
  "repository": "https://github.com/syncano/syncano-node/tree/master/packages/lib-js-core",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "syncano",
    "core",
    "socket",
    "endpoint"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf ./lib",
    "build": "npx tsc && npm run build:dts",
    "build:dts": "dts-bundle --name main --main ./lib/index.d.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:base": "cross-env NODE_ENV=test mocha --reporter spec --require babel-register",
    "test:unit": "npm run test:base -- test/unit/**/*.js",
    "lint": "tslint {src,test}/**/*.ts"
  },
  "dependencies": {
    "debug": "3.1.0",
    "form-data": "2.3.2",
    "lodash.get": "4.4.2",
    "lodash.merge": "4.6.1",
    "lodash.set": "4.3.2",
    "node-fetch": "2.1.2"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.1",
    "@types/lodash.get": "^4.4.3",
    "@types/lodash.merge": "^4.6.3",
    "@types/lodash.set": "^4.3.3",
    "@types/node": "^9.6.5",
    "@types/node-fetch": "^1.6.8",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cross-env": "5.1.4",
    "dts-bundle": "^0.7.3",
    "isparta": "4.0.0",
    "mocha": "5.1.0",
    "nock": "9.2.5",
    "rimraf": "2.6.2",
    "should": "13.2.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
