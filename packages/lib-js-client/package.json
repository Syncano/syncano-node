{
  "name": "@syncano/client",
  "version": "0.12.1",
  "description": "Interact with your Syncano Sockets.",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "unpkg": "dist/index.js",
  "author": "Syncano Inc.",
  "license": "MIT",
  "repository": "https://github.com/syncano/syncano-node/tree/master/packages/lib-js-client",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "syncano",
    "client",
    "endpoint"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npx tsc && npx webpack",
    "test": "jest",
    "test:unit": "jest",
    "lint": "tslint src/**/*.ts",
    "release:git-config": "git config --global user.email $GITHUB_EMAIL && git config --global user.name $GITHUB_NAME",
    "release:npm-token": "echo \"//registry.npmjs.org/:_authToken=${NPM_TOKEN}\nprogress=false\" > ~/.npmrc"
  },
  "dependencies": {
    "axios": "0.18.0"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/node": "9.6.7",
    "axios-mock-adapter": "1.15.0",
    "cross-env": "^5.1.4",
    "isparta": "^4.0.0",
    "jest": "22.4.3",
    "jsdom": "11.10.0",
    "should": "^13.2.1",
    "ts-jest": "22.4.5",
    "tslint": "5.10.0",
    "tslint-eslint-rules": "5.2.0",
    "typescript": "2.8.3",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3"
  }
}
