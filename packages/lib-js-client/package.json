{
  "name": "@syncano/client",
  "version": "0.15.0",
  "description": "Interact with your Syncano Sockets.",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "unpkg": "dist/index.js",
  "author": "Syncano Inc.",
  "license": "MIT",
  "repository": "https://github.com/syncano/syncano-node/tree/master/packages/lib-js-client",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "syncano",
    "client",
    "endpoint"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npx tsc && npx webpack",
    "test": "jest",
