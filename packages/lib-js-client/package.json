  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "axios-mock-adapter": "1.15.0",
    "cross-env": "^5.2.0",
    "isparta": "^4.1.0",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "should": "^13.2.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
